@model FormDetailViewModel
@{
	ViewData["Title"] = Model.Form.Title;
}
@if (Model.ReviewType.Equals("Praca Inżynierska") || Model.ReviewType.Equals("Praca Licencjacka"))
{
	@Html.Partial("Edits/BasicForm")
}
@if (Model.ReviewType.Equals("Praca Podyplomowa"))
{
	@Html.Partial("Edits/AdvanceForm")
}
@if (Model.ReviewType.Equals("Praca Magisterska"))
{
	@Html.Partial("Edits/MasterForm")
}