@model FormDetailViewModel
@{
	ViewData["Title"] = Model.Form.Title;
}
@if (Model.ReviewType.Equals("Praca Inzynierska") || Model.ReviewType.Equals("Praca Licencjacka"))
{
	@Html.Partial("View/BasicView")
}
@if (Model.ReviewType.Equals("Praca Doktorska"))
{
	@Html.Partial("View/AdvanceView")
}
@if (Model.ReviewType.Equals("Praca Magisterska"))
{
	@Html.Partial("View/MasterView")
}